    "prepublishOnly": "yarn test && yarn release"
  },
  "dependencies": {
    "ramda": "^0.27.0",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^25.2.4",
    "benchmark": "^2.1.4",
    "hyperid": "^2.0.3",
    "jest": "^25.2.4",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "standard": "^14.3.3",
    "timeout-as-promise": "^1.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "yarn-upgrade-all": "^0.5.2"
  },
  "files": [
    "dist/*",
    "src/*",
    "yarn.lock"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "yarn-upgrade-all": {
    "ignore": [
      "babel-core"
    ]
  }
