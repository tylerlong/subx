    "jest": "^23.6.0",
    "react": "^16.6.3",
    "react-test-renderer": "^16.6.3",
    "standard": "^12.0.1",
    "timeout-as-promise": "^1.0.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
